<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="15" author="Tobiasz">
        <createTable tableName="answer_session">
            <column name="id" type="BIGINT">
                <constraints primaryKey="true"/>
            </column>
            <column name="user_points" type="BIGINT"/>
            <column name="users_id" type="BIGINT">
                <constraints foreignKeyName="users_id" references="users(id)"/>
            </column>
            <column name="subject_id" type="BIGINT">
                <constraints foreignKeyName="subject_id" references="subject(id)"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>